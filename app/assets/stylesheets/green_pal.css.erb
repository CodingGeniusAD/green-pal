.recent-lawns-carousel {
  display: flex;
  gap: 30px;
  overflow-x: auto;
  padding: 20px 0;
  scrollbar-width: none;
  -ms-overflow-style: none;
}

.recent-lawns-carousel::-webkit-scrollbar {
  display: none;
}

.recent-lawn-card {
  width: 350px;
  max-width: 350px;
  background: white;

  overflow: hidden;
  flex-shrink: 0;
  word-wrap: break-word;
  word-break: break-word;
}

.card-image-container {
  position: relative;
  height: 150px;
  overflow: hidden;
  border-radius: 12px;
}

.card-house-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.card-logo {
  position: absolute;
  top: 15px;
  left: 15px;
 
}

.card-price-badge {
  position: absolute;
  top: 15px;
  right: 15px;
  background: #ff8c00;
  color: white;
  padding: 12px 16px;
  border-radius: 8px;
  font-size: 16px;
  font-weight: 700;
  text-align: center;
  line-height: 1.2;
}

.card-details {
  padding-top: 10px;
}

.card-service-title {
  margin-top: 10px;
  font-size: 18px;

  font-weight: 700;
  color: #374151;
  margin: 0 0 10px 0;
  line-height: 1.1;
  word-wrap: break-word;
  word-break: break-word;
  overflow-wrap: break-word;
}

.color-grey{
  color: #6B7280;
  font-size: 15px;
  font-weight: 400;
}

.card-location {
  display: flex;
  align-items: flex-start;
  gap: 8px;
  line-height: 1;
  margin-bottom: 12px;
  color: #6B7280;
  font-size: 14px;
  word-wrap: break-word;
  word-break: break-word;
  overflow-wrap: break-word;
}

.location-icon {
  flex-shrink: 0;
}

.card-info-item {
  display: flex;
  align-items: anchor-center;
  gap: 8px;
  margin-bottom: 8px;
  color: #000;
  font-size: 14px;
  font-weight: 500;
  word-wrap: break-word;
  word-break: break-word;
  overflow-wrap: break-word;
}

.info-icon {
  flex-shrink: 0;
}

.card-request-box {
  background: #EFF3F8;
  border-radius: 0px 24px 24px 24px;
  padding: 15px;
  margin-top: 15px;
}

.card-request-box p {
  color: #374151;
  font-size: 14px;
  line-height: 1.5;
  margin: 0;
  word-wrap: break-word;
  word-break: break-word;
  overflow-wrap: break-word;
}

.recent-lawns-pagination {
  display: flex;
  justify-content: center;
  gap: 8px;
  margin: 10px 0 30px 0;
}

.pagination-dot {
  width: 8px;
  height: 8px;
  border-radius: 50%;
  background: #d1d5db;
  cursor: pointer;
  transition: background-color 0.3s ease;
}

.pagination-dot.active {
  background: #374151;
}

.recent-lawns-cta {
  text-align: center;
}

.see-prices-recent-btn {
  background: linear-gradient(135deg, #ff8c00, #ff6b35);
  color: white;
  border: none;
  width: 100%;
  border-radius: 8888px;
  padding: 16px 32px;
  font-size: 18px;
  font-weight: 600;
  cursor: pointer;
  transition: all 0.3s ease;
  
}

.see-prices-recent-btn:hover {
  transform: translateY(-2px);
  box-shadow: 0 6px 20px rgba(255, 140, 0, 0.4);
}

@media (max-width: 768px) {
  .recent-lawns-mowed {
    padding: 60px 0;
  }
  
 
  .recent-lawns-subtitle {
    font-size: 16px;
  }
  
  .recent-lawn-card {
    width: 300px;
    max-width: 300px;
  }
  
  /* .card-details {
    padding: 20px;
  }
   */
 
  
  .see-prices-recent-btn {
    padding: 14px 28px;
    font-size: 16px;
  }
}

@media (max-width: 480px) {
  .recent-lawns-mowed {
    padding: 40px 0;
  }
  

  
  .recent-lawns-subtitle {
    font-size: 15px;
  }
  
  .recent-lawns-carousel {
    gap: 20px;
  }
  
  .recent-lawn-card {
    width: 280px;
    max-width: 280px;
  }
  
  /* .card-details {
    padding: 18px;
  } */
  
 
  
  .card-info-item {
    font-size: 13px;
  }
  
  .card-request-box p {
    font-size: 13px;
  }
  
  .see-prices-recent-btn {
    padding: 12px 24px;
    font-size: 15px;
  }
}

@media (max-width: 360px) {

  
  
  .recent-lawn-card {
    width: 260px;
    max-width: 260px;
  }
  
  /* .card-details {
    padding: 15px;
  } */
  

  .card-info-item {
    font-size: 12px;
  }
  
  .card-request-box p {
    font-size: 12px;
  }
}



