/* container you add around the paragraph/content */
.read-more-glass {
  position: relative;
  /* create stacking context (helps consistency) */
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
}

/* make the real content sit below the overlay */
.read-more-glass > * {
  position: relative;
  z-index: 1;
}

/* the glass fade overlay at the bottom */
.read-more-glass::after {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 25px;                    
  z-index: 2;                      
  pointer-events: none;
  background: linear-gradient(
    to bottom,
    rgba(255,255,255,0) 0%,
    rgba(255,255,255,0.30) 100%
  );
  /* the actual frosted glass effect */
  backdrop-filter: blur(2px) saturate(120%);
  -webkit-backdrop-filter: blur(2px) saturate(120%); /* Safari */
  /* optional soft edge if your container has rounded corners */
  border-bottom-left-radius: inherit;
  border-bottom-right-radius: inherit;
}

/* Fallback: when backdrop-filter is unsupported, make the fade a bit stronger */
@supports not ((-webkit-backdrop-filter: blur(2px)) or (backdrop-filter: blur(2px))) {
  .read-more-glass::after {
    background: linear-gradient(
      to bottom,
      rgba(255,255,255,0) 0%,
      rgba(255,255,255,0.60) 100%
    );
  }
}
